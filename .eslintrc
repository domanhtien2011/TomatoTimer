{
    "parser": "babel-eslint",
    "extends": [
        "standard",
        "standard-react"
    ],
    "plugins": [
        "babel",
        "react",
        "promise",
        "import"
    ],
    "env": {
        "browser": true,
        "jest": true
    },
    "globals": {
        "__DEV__": false,
        "__TEST__": false,
        "__PROD__": false,
        "__COVERAGE__": false
    },
    "rules": {
        "key-spacing": 0,
        "jsx-quotes": [
            2,
            "prefer-single"
        ],
        "max-len": [
            2,
            120,
            2
        ],
        "object-curly-spacing": [
            2,
            "always"
        ],
        "no-duplicate-imports": 0,
        "react/no-unused-prop-types": 0,
        "import/no-duplicates": 2,
        "no-console": "error"
    }
}